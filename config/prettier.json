{
    "trailingComma": "es5",
    "arrowParens": "always",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "printWidth": 120,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.jsonc",
            "options": {
                "parser": "jsonc",
                "trailingComma": "none"
            }
        }
    ]
}
